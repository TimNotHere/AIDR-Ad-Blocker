{
    "manifest_version": 2,
    "name": "AIDR",
    "description": "This extension blocks ads on websites to improve user experience.",
    "version": "1.1",
    "permissions": ["webRequest", "webRequestBlocking", "*://*/*"],
    "icons": {
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "128": "icon128.png"
        }
    },
    "background": {
        "scripts": ["block.js", "background.js"]
    }
}
